{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "react/prop-types": "off",
    "react/jsx-key": "off",
    "react/react-in-jsx-scope": "off"
  },
  "overrides": [
    {
      "files": ["pages/api/*.js"],
      "rules": {
        "no-unused-vars": "off"
      }
    }
  ]
}
